   "source": [
    "import pandas as pd\n",
    "import json\n",
    "import re\n",
    "\n",
    "# Read the CSV file into a DataFrame\n",
    "df = pd.read_csv('./Datasets/Cleaned Grab SG Restaurants.csv')\n",
    "\n",
    "# Access the 'opening_hours' column\n",
    "jsonOpeningHours = df['opening_hours']\n",
    "\n",
    "# Define a function to calculate total opening time\n",
    "def calculate_total_opening_time(opening_hours_json):\n",
    "    # Load the JSON string into a dictionary\n",
    "    opening_hours_dict = json.loads(opening_hours_json)\n",
    "    \n",
    "    # Extract opening hours for each day excluding 'open' and 'displayedHours' keys\n",
    "    opening_hours = [hours for day, hours in opening_hours_dict.items() if day not in ['open', 'displayedHours']]\n",
    "    \n",
    "    # Extract time intervals for each day and sum the opening durations\n",
    "    total_opening_time = sum(\n",
    "        (int(closing_time[:2]) - int(opening_time[:2])) + (int(closing_time[3:]) - int(opening_time[3:])) / 60\n",
    "        for hours in opening_hours if hours != \"Closed\"\n",
    "        for opening_time, closing_time in re.findall(r'(\\d{2}:\\d{2})-(\\d{2}:\\d{2})', hours)\n",
    "    )\n",
    "    \n",
    "    return total_opening_time\n",
    "\n",
    "# Apply the function to each element of the 'opening_hours' column\n",
    "df['total_opening_time_hours'] = jsonOpeningHours.apply(calculate_total_opening_time)\n",
    "\n",
    "# Now df['total_opening_time_hours'] contains the total opening time for each entry\n",
    "# You can further process or analyze this data as needed\n"
   ]
